{
    "name": "tesprima",
    "description": "Typed Esprima",
    "main": "tesprima.js",
    "version": "0.0.1-dev",
    "engines": {
        "node": ">=0.4.0"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/ikarienator/tesprima.git"
    },
    "devDependencies": {
        "jslint": "~0.1.9",
        "istanbul": "~0.1.27",
        "complexity-report": "~0.6.1",
        "esprima": "~1.0.2"
    },
    "scripts": {
        "test": "node node_modules/esprima/test/run.js && npm run-script coverage && npm run-script complexity",
        "coverage": "npm run-script analyze-coverage && npm run-script check-coverage",
        "analyze-coverage": "node node_modules/esprima/node_modules/istanbul/lib/cli.js cover node_modules/esprima/test/runner.js",
        "check-coverage": "node node_modules/esprima/node_modules/istanbul/lib/cli.js check-coverage --statement -7 --branch -22 --function 100",

        "complexity": "npm run-script analyze-complexity && npm run-script check-complexity",
        "analyze-complexity": "node node_modules/esprima/tools/list-complexity.js",
        "check-complexity": "node node_modules/esprima/node_modules/complexity-report/src/cli.js --maxcc 21 --silent -l -w tesprima.js",

        "benchmark": "node node_modules/esprima/test/benchmarks.js",
        "benchmark-quick": "node node_modules/esprima/test/benchmarks.js quick"
    }
}
